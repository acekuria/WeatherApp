{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "prettier/prettier": "warn",
    "object-shorthand": "off",
    "class-methods-use-this": "off",
    "no-restricted-syntax": "off",
    "consistent-return": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off"
  }
}
